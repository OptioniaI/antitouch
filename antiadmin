local Admins = {"Amphorax", "udakotau", "kxt3n", "Xeptly", "feeteoee", "wildteds", "haos_garry", "10AELMO", "Duckscy", "firelittenlols", "flamozu", "Suggestionz", "Aokwkkao", "Undistanced", "Regionality", "ladsmaca", "lunstia", "macalads"}

for i,v in pairs (game.Players:GetPlayers()) do
    if table.find(Admins, v.Name) then
        game.Players.LocalPlayer:Kick('You have been kicked from AntiAdmin.')
    else
    --print('no admin')
    end
end

game.Players.PlayerAdded:connect(function(plr)
    if table.find(Admins, plr.Name) then
        game.Players.LocalPlayer:Kick('You have been kicked from AntiAdmin.')
        else
     --   Notify('Alert', 'no admin yet')
    end
end)
